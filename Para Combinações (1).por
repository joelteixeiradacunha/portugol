programa {
  funcao inicio() {
    inteiro contadorUm, contadorDois, contadorTres
    para(contadorUm = 1; contadorUm <= 3; contadorUm ++){
      para(contadorDois = 1; contadorDois <=3; contadorDois ++){
        para(contadorTres = 1; contadorTres <=3; contadorTres ++){
          escreva(" " + contadorUm + ", " + contadorDois + ", "  + contadorTres + "|")
        }
      }
        
    }
  }
}
