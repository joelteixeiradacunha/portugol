programa {
  funcao inicio() {
    inteiro valor, contador

    escreva("Digite um valor menor que 20: ")
    leia(valor)

    para( contador = 0; contador <= valor; contador ++){
      escreva(" "+ contador + " ")
    }
    escreva("\n")
